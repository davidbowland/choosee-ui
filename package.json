{
  "name": "choosee-ui",
  "version": "1.34.0",
  "description": "UI for choosee",
  "author": "David Bowland",
  "engines": {
    "node": "^18.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbowland/choosee-ui.git"
  },
  "bugs": {
    "url": "https://github.com/davidbowland/choosee-ui/issues"
  },
  "homepage": "https://github.com/davidbowland/choosee-ui#readme",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rm -rf .cache coverage public && npm ci && gatsby clean",
    "deploy": "DEVELOPMENT=true npm run build && ./scripts/deploy.sh",
    "lint": "prettier --write . && eslint --fix . --resolve-plugins-relative-to .",
    "prepare": "if [ \"$NODE_ENV\" != \"production\" ]; then husky install ; fi",
    "start": "gatsby develop",
    "serve": "npm run build && gatsby serve",
    "test": "jest --colors",
    "typecheck": "tsc --noEmit",
    "update": "ncu --doctor --target minor --upgrade && npm audit fix --audit-level=none && npm run test && npm dedupe"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^4.6.5",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^4.5.8",
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "^5.0.0-alpha.151",
    "@mui/material": "^5.14.16",
    "@types/grecaptcha": "^3.0.6",
    "aws-amplify": "^5.3.11",
    "babel-plugin-styled-components": "^2.1.4",
    "crypto-browserify": "^3.12.0",
    "fast-json-patch": "^3.1.1",
    "gatsby": "^5.12.9",
    "gatsby-legacy-polyfills": "^3.12.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^3.12.3",
    "gatsby-plugin-mdx": "^5.12.3",
    "gatsby-plugin-react-helmet": "^6.12.0",
    "gatsby-plugin-sass": "^6.12.3",
    "gatsby-plugin-sharp": "^5.12.3",
    "gatsby-plugin-sitemap": "^6.12.3",
    "gatsby-plugin-styled-components": "^6.12.0",
    "gatsby-source-filesystem": "^5.12.1",
    "gatsby-transformer-sharp": "^5.12.3",
    "graphql": "^16.8.1",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-material-ui-carousel": "^3.4.2",
    "stream-browserify": "^3.0.0",
    "styled-components": "^5.3.11",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.23.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.7",
    "@types/node": "^18.18.8",
    "@types/react": "^18.2.35",
    "@types/react-dom": "^18.2.14",
    "@types/react-helmet": "^6.1.8",
    "@types/styled-components": "^5.1.29",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "babel-jest": "^29.7.0",
    "babel-preset-gatsby": "^3.12.1",
    "eslint-config-prettier": "^8.10.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.3.0",
    "msw": "^1.3.2",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix --resolve-plugins-relative-to .",
      "jest --colors --findRelatedTests --coverageThreshold '{}' --passWithNoTests"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
